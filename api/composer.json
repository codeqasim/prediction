{
    "name": "codeqasim/v10",
    "description": "",
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.2",
        "catfan/medoo": "^2.2",
        "phpmailer/phpmailer": "^6.10",
        "qaxim/php-router": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "AppRouter\\": "vendor/qaxim/php-router/src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "secure-http": false
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "http://repo.packagist.org"
        }
    }
}
